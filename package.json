{
  "name": "sudoku",
  "version": "0.0.2",
  "description": "Sudoku library in TypeScript.",
  "main": "./dist/src/index.js",
  "typings": "./dist/src/index.d.ts",
  "typescript": {
    "main": "./src/index.ts"
  },
  "scripts": {
    "devtools": "npm install && typings install",
    "tsc": "tsc",
    "lint": "tslint -c tslint.json ./src/**/*.ts",
    "mocha": "mocha --reporter dot ./dist/**/*.spec.js",
    "istanbul": "istanbul cover node_modules/mocha/bin/_mocha dist/tests -- -R dot --recursive",
    "test": "npm run tsc && npm run istanbul",
    "build": "npm run lint && npm run tsc",
    "all": "npm run lint && npm run tsc && npm run istanbul"
  },
  "author": "Marcus Bodin <bodinaren@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "btypescript": "file:..\\bTypeScript",
    "rxjs": "^6.2.1"
  },
  "devDependencies": {
    "browserify": "^16.2.2",
    "browserify-umdify": "^1.0.3",
    "chai": "^4.1.2",
    "intern": "^4.2.0",
    "mocha": "^5.2.0",
    "remap-istanbul": "^0.11.1",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2",
    "uglify-js": "^3.4.0",
    "uglifyify": "^5.0.0"
  }
}
